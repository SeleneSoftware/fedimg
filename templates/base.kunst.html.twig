<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        <meta name="viewport" content="width=device-width,initial-scale=1">
        <meta name="generator" content="RSD 5.0.3490">
        <script>document.createElement( "picture" );</script>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('kunst') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('kunst') }}
        {% endblock %}
        <link rel="stylesheet" href="https://fonts.bunny.net/css?family=Inter:400,500">
        <style>::selection { background-color: #e0261b; color: white }</style>
    </head>
    <body>

        {% block navbar %}
        <nav class="container navbar">
            {% if app.user %}
                <a class="link-text logo-nav-link" href="{{ path('feed', {'username': app.user.getUsername}) }}">{{ app.user.getUsername }}</a><br><a class="link-text logo-nav-link" href="{{ path('profile') }}">Profile</a>
            {% endif %}
            <div class="container navigation-links"><a class="glyph mobile-nav-button close-button-cross" href="#"><i class="coffeecup-icons-cross2"></i></a>
                <a class="link-text navlink work-link-mobile-only" href="index.html#projects" onclick="location.reload();">Contact</a>
                <a class="link-text navlink work-link-mobile-only" href="index.html#projects" onclick="location.reload();">Instagram</a>
                <a class="link-text navlink work-link-desktop-only" href="index.html#projects" data-scroll="">Contact</a>
                <a class="link-text navlink work-link-desktop-only" href="index.html#projects" data-scroll="">Instagram</a>
            </div>
            <div class="html-element navigation-code"><script>
                    document.addEventListener("DOMContentLoaded", function(event) {
                        $('.mobile-nav-button').click(function(e){
                            $('.navigation-links').toggleClass('show-menu');
                            $('body').css('overflow', $('.navigation-links').hasClass('show-menu') ? 'hidden' : 'auto');
                        });
                    });
            </script></div><a class="glyph mobile-nav-button" href="#!"><i class="coffeecup-icons-menu7"></i></a>
        </nav>
        {% endblock %}

        {% block hero %}
        <section class="container hero-section" style="background-image: linear-gradient(25deg, rgba(26, 26, 26, 1) 0%, rgba(0, 0, 0, 0) 50%), url('https://picsum.photos/1280/865')">
            <div class="container container-hero-content">
                <h1 class="heading-1">{{ app.user.getProfile.getHeading|default('Welcome To My FedImg Feed') }}<br></h1>
            </div>
        </section>
        {% endblock %}

        {% block feed %}
        <section class="container projects-section" id="projects">
            <div class="container container-projects-title">
                <h2 class="heading-2">Last projects</h2>
            </div>
            <div class="container sd-two-cols two-column-pictures">
                <div class="container container-center-picture">
                    <div class="responsive-picture picture-project">
                        <picture>
                            <img alt="Placeholder Picture" width="1920" height="1280" src="{{ asset('/images/pexels-circling-sea-5625832.jpg') }}" loading="lazy">
                        </picture>
                    </div>
                    <h3 class="heading-3">Girl &amp; Wolf – 2020</h3>
                </div>
                <div class="container container-center-picture">
                    <div class="responsive-picture picture-project">
                        <picture>
                            <img alt="Placeholder Picture" width="1280" height="1919" src="./images/pexels-aleksandar-pasaric-6560708.jpg" loading="lazy">
                        </picture>
                    </div>
                    <h3 class="heading-3">Skyfall – 2020</h3>
                </div>
            </div>
            <div class="container sd-two-cols two-column-pictures">
                <div class="container container-center-picture">
                    <div class="responsive-picture picture-project">
                        <picture>
                            <img alt="Placeholder Picture" width="1920" height="2880" src="./images/inside-weather-Uxqlfigh6oE-unsplash.jpg" loading="lazy">
                        </picture>
                    </div>
                    <h3 class="heading-3">Ibiza – 2019</h3>
                </div>
                <div class="container container-center-picture">
                    <div class="responsive-picture picture-project">
                        <picture>
                            <img alt="Placeholder Picture" width="1280" height="1920" src="./images/pexels-monstera-6373480.jpg" loading="lazy">
                        </picture>
                    </div>
                    <h3 class="heading-3">Stockholm – 2019</h3>
                </div>
            </div>
            <div class="container sd-three-cols three-column-pictures">
                <div class="container container-center-picture">
                    <div class="responsive-picture picture-project">
                        <picture>
                            <img alt="Placeholder Picture" width="1280" height="1707" src="./images/pexels-maxim-kovalev-8647264.jpg" loading="lazy">
                        </picture>
                    </div>
                    <h3 class="heading-3">The Void – 2019</h3>
                </div>
                <div class="container container-center-picture">
                    <div class="responsive-picture picture-project">
                        <picture>
                            <img alt="Placeholder Picture" width="1280" height="1807" src="./images/pexels-rachel-claire-4577793.jpg" loading="lazy">
                        </picture>
                    </div>
                    <h3 class="heading-3">Nomads – 2019</h3>
                </div>
                <div class="container container-center-picture">
                    <div class="responsive-picture picture-project">
                        <picture>
                            <img alt="Placeholder Picture" width="1280" height="1919" src="./images/pexels-ave-calvar-martinez-4092124.jpg" loading="lazy">
                        </picture>
                    </div>
                    <h3 class="heading-3">Cold Waters – 2018</h3>
                </div>
            </div>
        </section>
        {% endblock %}

        {# Why am I keeping this here?  Someday, I might want videos.
        <section class="container video-section">
            <div class="container container-projects-title">
                <h2 class="heading-2">Last Film</h2>
            </div>
            <div class="container bg-content-size-container">
                <video class="content-size-video" autoplay="" loop="loop" muted="">
                    <source src="./video/pexels-mikhail-nilov-6981410.mp4" type="video/mp4">
                </video>
            </div>
        </section> #}

        {% block feedTwoPic %}
        <div class="container sd-two-cols two-column-pictures">
            <div class="container container-center-picture">
                <div class="responsive-picture picture-project">
                    <picture>
                        <img alt="Placeholder Picture" width="1280" height="853" src="./images/pexels-mentatdgt-987230.jpg" loading="lazy">
                    </picture>
                </div>
                <h3 class="heading-3">Girl &amp; Wolf – 2020</h3>
            </div>
            <div class="container container-center-picture">
                <div class="responsive-picture picture-project">
                    <picture>
                        <img alt="Placeholder Picture" width="1280" height="853" src="./images/pexels-adrien-olichon-2387793.jpg" loading="lazy">
                    </picture>
                </div>
                <h3 class="heading-3">Volcanic Lands – 2016</h3>
            </div>
        </div>
        {% endblock %}

        {% block footer %}
        <footer class="container footer">
            <div class="container footer-3-columns">
                <div class="container description-logos-footer">
                    <h4 class="headline-footer">Footer paragraph desciption. I'm Emily Kunst, a photographer &amp; filmmaker based in Berlin.</h4>
                </div>
                <div class="container container-links-footer-wrapper-1">
                    <a class="link-text link-footer" href="#">Contact</a>
                    <a class="link-text link-footer" href="#">Instagram</a>
                </div>
                <div class="container container-direction-footer">
                    <p class="paragraph direction-footer">Valencia 190<br>08011 Barcelona</p>
                    <a class="link-text link-footer" href="#">info@yourbrand.com</a>
                </div>
            </div>
        </footer>
        {% endblock %}

        <script src="https://cdn.jsdelivr.net/gh/cferdinandi/smooth-scroll@15/dist/smooth-scroll.polyfills.min.js"></script>

        <script>
            var scroll = new SmoothScroll('a[href*="#"][data-scroll]', {
                speed: 750
            });
        </script>
    </body>
</html>
